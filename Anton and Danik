
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int t;
    cin>>t;
    int a=0;
    int d=0;
    while(t--){
        char c;
        cin>>c;
        if(c=='A') a++;
        if(c=='D') d++;
    }
    if(a>d){
        cout<<"Anton";
    }
    else if(a==d){
        cout<<"Friendship";
    }
    else{
        cout<<"Danik";
    }
    
    
    
}
