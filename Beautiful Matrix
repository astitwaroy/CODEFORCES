#include <iostream>
 
using namespace std;
 
int main()
{
    int r,c;
    int arr[5][5];
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            cin>>arr[i][j];
            if(arr[i][j]==1){
                r=i;
                c=j;
            }
        }
    }
    int ways = abs(2-r)+abs(2-c);
    cout<<ways;
    return 0;
}
