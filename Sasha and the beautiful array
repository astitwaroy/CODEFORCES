#include <bits/stdc++.h> 
using namespace std; 
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        vector<int>v;
        for(int i=0;i<n;i++){
            int vec;
            cin>>vec;
            v.push_back(vec);
        }
        sort(v.begin(), v.end());
        int a=v[n-1]-v[0];
        cout<<a<<endl;
        
    }
}
