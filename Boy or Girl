#include<bits/stdc++.h>
using namespace std;
int main(){
    char s[101];
    cin>>s;
    int l=strlen(s);
    int count =0;
    sort(s,s+l);
    for(int i=0;i<l;i++){
        if(s[i]!=s[i+1]){
            count++;
        }
        
    }
    if(count%2==0){
        cout<<"CHAT WITH HER!";
    }
    else{
        cout<<"IGNORE HIM!";
    }
 
    
}
