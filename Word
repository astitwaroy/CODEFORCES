#include <bits/stdc++.h>

using namespace std;

int main() {
	string s;
	cin>>s;
	int up=0,low=0;
	for(int i=0;i<s.size();i++){
	    if(isupper(s[i])){
	        up++;
	    }else{
	        low++;
	    }
	}
	if(up>low){
	    string u;
	    for(int i=0;i<s.size();i++){
	        u=toupper(s[i]);
	        cout<<u;
	    }
	}else{
	    string u;
	    for(int i=0;i<s.size();i++){
	        u=tolower(s[i]);
	        cout<<u;
	    }
	}
	
	return 0;}
